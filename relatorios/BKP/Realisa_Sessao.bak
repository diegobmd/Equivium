	<queryString><![CDATA[SELECT
     REALIZA_SESSAO."RSE_CODIGO" AS REALIZA_SESSAO_RSE_CODIGO,
     REALIZA_SESSAO."RSE_SIGLA" AS REALIZA_SESSAO_RSE_SIGLA,
     REALIZA_SESSAO."RSE_NOME" AS REALIZA_SESSAO_RSE_NOME,
     REALIZA_SESSAO."RSE_DATA" AS REALIZA_SESSAO_RSE_DATA,
     PARTICIPANTES."PAR_NOME" AS PARTICIPANTES_PAR_NOME,
     PESQUISADORES."PES_NOME" AS PESQUISADORES_PES_NOME
FROM
     "PUBLIC"."PARTICIPANTES" PARTICIPANTES INNER JOIN "PUBLIC"."REALIZA_SESSAO" REALIZA_SESSAO ON PARTICIPANTES."PAR_CODIGO" = REALIZA_SESSAO."RSE_PARTICIPANTE"
     INNER JOIN "PUBLIC"."PESQUISADORES" PESQUISADORES ON REALIZA_SESSAO."RSE_PESQUISADOR" = PESQUISADORES."PES_CODIGO"
 $P!{sql}]]></queryString>

