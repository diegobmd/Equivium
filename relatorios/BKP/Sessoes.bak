	<queryString><![CDATA[SELECT
     SESSOES."SES_CODIGO" AS SESSOES_SES_CODIGO,
     SESSOES."SES_SIGLA" AS SESSOES_SES_SIGLA,
     SESSOES."SES_NOME" AS SESSOES_SES_NOME,
     REFORCOS."REF_NOME" AS REFORCOS_REF_NOME,
     FASES."FAS_CODIGO" AS FASES_FAS_CODIGO,
     FASES."FAS_SIGLA" AS FASES_FAS_SIGLA,
     FASES."FAS_NOME" AS FASES_FAS_NOME
FROM
     "PUBLIC"."REFORCOS" REFORCOS RIGHT OUTER JOIN "PUBLIC"."SESSOES" SESSOES ON REFORCOS."REF_CODIGO" = SESSOES."SES_REFORCO_FIM"
     LEFT OUTER JOIN "PUBLIC"."SESSAO_FASES" SESSAO_FASES ON SESSOES."SES_CODIGO" = SESSAO_FASES."SES_FAS_SESSAO"
     LEFT OUTER JOIN "PUBLIC"."FASES" FASES ON SESSAO_FASES."SES_FAS_FASE" = FASES."FAS_CODIGO"
$P!{sql}]]></queryString>

